{
  "env": {
    "development": {
      "presets": [
        ["es2015", { "modules": "commonjs", "loose": true }],
        "preact",
        "stage-1"
      ]
    },
    "rollup": {
      "presets": [
        ["es2015", { "modules": false, "loose": true }],
        "preact",
        "stage-1"
      ],
      "plugins": [
        "external-helpers"
      ]
    },
    "jsnext": {
      "presets": [
        ["es2015", { "modules": false, "loose": true }],
        "preact",
        "stage-1"
      ]
    }
  }
}
